<?xml version="1.0" ?>

<!-- pressureForces.report.xml
This report is working just with the last compute pressure forces, therefore, if
pressure forces over several bodies are required, you'll need to create your own
reporter.
-->

<sphInput>
    <Reports>
        <Report type="screen" name="Pressure forces" fields="pressureForces_F;pressureForces_M" color="red" bold="false"/>
        <Report type="file" name="Pressure forces file" fields="t, pressureForces_F, pressureForces_M" path="PressureForces.dat"/>
    </Reports>
</sphInput>
 
